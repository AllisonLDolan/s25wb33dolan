extends layout

block content
  h1= title

  - let image_names = ['image1.jpg', 'image2.jpg', 'image3.png', 'image4.jpg', 'image5.jpg']

  - for (let i = 0; i < 3; i++) {
    - const randomIndex = Math.floor(Math.random() * 5)
    - const image = image_names[randomIndex]
    img(
      src=`/images/${image}`,
      alt=`${image.split('.')[0]}`,
      width="300",
      height="300"
    )
  - }